{
    "name": "devgroup/yii2-frontend-utils",
    "description": "Various frontend utilities for yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","frontend","frontend-monster"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Alexander Kozhevnikov",
            "email": "b37hr3z3n@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0.6",
        "devgroup/yii2-tag-dependency-helper": "^1.3.0",
        "devgroup/yii2-polyglot": "~1.0.1",
        "devgroup/bh": "^4.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "phpunit/dbunit": "~1.0",
        "yiisoft/yii2-debug": "^2.0@dev",
        "yiisoft/yii2-gii": "^2.0@dev"
    },
    "autoload": {
        "psr-4": {
            "DevGroup\\Frontend\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevGroup\\Frontend\\Tests\\": "tests/"
        }
    }
}
