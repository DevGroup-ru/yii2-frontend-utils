{
    "name": "devgroup/yii2-frontend-utils",
    "description": "Various frontend utilities for yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","frontend","frontend-monster"],
    "license": "MIT",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Alexander Kozhevnikov",
            "email": "b37hr3z3n@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "^2.0.6",
        "devgroup/yii2-tag-dependency-helper": "^1.3.0",
        "devgroup/yii2-polyglot": "~1.0.1"
    },
    "require-dev": {
        "devgroup/dotplant-dev": "*"
    },
    "autoload": {
        "psr-4": {
            "DevGroup\\Frontend\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevGroup\\Frontend\\Tests\\": "tests/"
        }
    },
    "extra": {
      "yii2-extension": {
        "name": "Frontend utils",
        "name_ru": "Frontend утилиты",
        "iconUrl": "https://st-1.dotplant.ru/images/ext-sample.png",
        "description": "Various frontend components for yii2",
        "description_ru": "Различные компоненты для yii2"
      }
    }
}
